    "eslint-plugin-jest": "27.0.4",
    "jest": "29.0.3",
    "nodemon": "2.0.19",
    "prettier": "2.7.1",
    "rimraf": "3.0.2",
    "semantic-release": "19.0.5",
    "typescript": "4.8.3"
  },
  "engines": {
    "node": ">=14.0.0"
  }
}
