  "version": "0.0.0",
  "author": {
    "name": "Alexander Daza",
    "email": "dev.alexander.daza@gmail.com",
    "url": "https://www.alexanderdaza.com"
  },
  "contributors": [
    {
      "name": "Alexander Daza",
      "email": "dev.alexander.daza@gmail.com",
      "url": "https://www.alexanderdaza.com"
    }
  ],
  "license": "MIT",
  "main": "./build/index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/devalexanderdaza/whatsappi.git"
  },
  "bugs": {
    "url": "https://github.com/devalexanderdaza/whatsappi/issues"
  },
  "homepage": "https://github.com/devalexanderdaza/whatsappi#readme",
  "files": [
    "build"
  ],
  "keywords": [
    "whatsapp",
    "api",
    "baileys",
    "typescript",
    "swc",
    "nodejs",
    "library",
    "wrapper",
    "easy",
    "lib",
    "integration",
    "whatsapp",
    "whatsappjs",
    "whatsapp.js",
    "whatsapp-api",
    "whatsapp-api-wrapper",
    "whatsapp-api-lib",
    "whatsapp-api-library",
    "whatsapp-api-wrapper",
    "whatsapp-api-wrapper-lib",
    "whatsapp-api-wrapper-library",
    "whatsapp-api-wrapper-library-lib",
    "whatsapp-api-wrapper-library-library",
    "multidevice",
    "multidevice-whatsapp",
    "whatsapp-bot",
    "whatsapp-bot-lib",
    "whatsapp-bot-library",
    "whatsapp-bot-wrapper",
    "whatsapp-client",
    "package",
    "github"
  ],
  "scripts": {
    "start": "node build/index.js",
    "dev": "nodemon",
    "develop": "node -r @swc-node/register ./src/index.ts",
    "test": "jest --passWithNoTests",
    "test:watch": "jest --watch",
    "clean": "rimraf build",
    "build": "tsc -p tsconfig.build.json",
    "build:watch": "tsc -w -p tsconfig.build.json",
    "build:swc": "swc ./src -d build",
    "build:swc:watch": "swc ./src -d build -w",
    "lint": "eslint ./src --ext .ts",
    "prettier": "prettier --write './src/**/*.{ts,js,json}'",
    "release": "semantic-release"
  },
  "dependencies": {
    "@adiwajshing/baileys": "^4.4.0",
    "awesome-phonenumber": "^3.3.0",
    "axios": "^0.27.2",
    "file-type": "^18.0.0",
    "formdata-node": "^5.0.0",
    "human-readable": "^0.2.1",
    "jsdom": "^20.0.0",
    "lodash": "^4.17.21",
    "moment-timezone": "^0.5.37",
    "node-fetch": "^3.2.10",
    "node-cache": "^5.1.2",
    "node-cron": "^3.0.1",
    "perf_hooks": "^0.0.1",
    "pino": "^8.5.0",
    "pino-pretty": "^9.1.0",
    "qrcode": "1.5.1",
    "uuid": "^9.0.0",
    "wa-sticker-formatter": "^4.3.2"
  },
  "peerDependencies": {
    "jimp": "^0.16.1",
    "link-preview-js": "^2.1.13",
    "qrcode-terminal": "^0.12.0"
  },
  "peerDependenciesMeta": {
    "jimp": {
      "optional": true
    },
    "qrcode-terminal": {
